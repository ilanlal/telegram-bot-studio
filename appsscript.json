{
  "timeZone": "America/New_York",
  "exceptionLogging": "STACKDRIVER",
  "runtimeVersion": "V8",
  "webapp": {
    "executeAs": "USER_DEPLOYING",
    "access": "ANYONE_ANONYMOUS"
  },
  "oauthScopes": [
    "https://www.googleapis.com/auth/script.locale",
    "https://www.googleapis.com/auth/spreadsheets.currentonly",
    "https://www.googleapis.com/auth/script.external_request",
    "https://www.googleapis.com/auth/script.scriptapp",
    "https://www.googleapis.com/auth/userinfo.email"
  ],
  "addOns": {
    "common": {
      "name": "Telegram Bot Studio",
      "logoUrl": "https://raw.githubusercontent.com/ilanlal/telegram-bot-studio/main/assets/google-workspace-marketplace/192x192.png",
      "layoutProperties": {
        "primaryColor": "#1976d2",
        "secondaryColor": "#0f9d58"
      },
      "openLinkUrlPrefixes": [
        "https://raw.githubusercontent.com/ilanlal/",
        "https://www.github.com/ilanlal/",
        "https://github.com/ilanlal/",
        "https://www.easyadm.com",
        "https://easyadm.com",
        "https://t.me/",
        "https://telegram.me/",
        "https://api.telegram.org/"
      ],
      "homepageTrigger": {
        "runFunction": "Plugins.Home.OnLoad"
      },
      "useLocaleFromApp": true,
      "universalActions": [
        {
          "label": "Profile",
          "runFunction": "Plugins.UserProfile.OnLoad"
        },
        {
          "label": "Settings",
          "runFunction": "Plugins.Settings.OnLoad"
        },
        {
          "label": "Help",
          "runFunction": "Plugins.Home.OnHelp"
        },
        {
          "label": "About",
          "runFunction": "Plugins.Home.OnAbout"
        },
        {
          "label": "Visit Bot Studio",
          "openLink": "https://github.com/ilanlal/telegram-bot-studio#readme"
        }
      ]
    },
    "sheets": {}
  },
  "urlFetchWhitelist": [
    "https://api.telegram.org/",
    "https://t.me/",
    "https://telegram.me/",
    "https://raw.githubusercontent.com/ilanlal/",
    "https://www.github.com/ilanlal/",
    "https://github.com/ilanlal/",
    "https://www.easyadm.com/",
    "https://easyadm.com/"
  ]
}